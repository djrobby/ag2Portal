<% content_for :title, "aGestiona2 | ag2Payments" %>
<% content_for :icon, "/assets/icon/ag2Payments.png" %>
<% content_for :alt, "ag2Payments" %>

<!-- Breadcrumb -->
<ul class="breadcrumb">
  <li>
    <a href="<%= main_app.root_path %>"><i class="icon-home"></i></a><span class="divider">></span>
  </li>
  <li>
    <a href="<%= ag2_purchase.root_path %>">ag2Purchase</a><span class="divider">></span>
  </li>
  <% if @supplier_payments %>
  <li>
    ag2Payments
  </li>
  <% end %>
  <% if @supplier_payment %>
  <li>
    <a href="<%= supplier_payments_path %>">ag2Payments</a><span class="divider">></span>
  </li>
  <% if @breadcrumb == 'read' || @breadcrumb == 'update' %>
  <%=t :"ag2_purchase.breadcrumb.supplier_payments.item" %>
  <% end %>
  <% if @breadcrumb == 'create' %>
  <li>
    <%= t :"breadcrumb.new" %>
  </li>
  <% end %>
  <!-- Records navigator -->
  <% if @breadcrumb == 'read' %>
  <li>
    <span class="divider"></span>
  </li>
  <li>
    <%= link_to @supplier_payment.to_first, :class => 'notice-icon-button', :id => 'first', :title => I18n.t(:first) do %>
    <i class="icon-fast-backward"></i>
    <% end %>
  </li>
  <li>
    <%= link_to @supplier_payment.to_prev, :class => 'notice-icon-button', :id => 'prev', :title => I18n.t(:prev) do %>
    <i class="icon-backward"></i>
    <% end %>
  </li>
  <li>
    <%= link_to @supplier_payment.to_next, :class => 'notice-icon-button', :id => 'next', :title => I18n.t(:next) do %>
    <i class="icon-forward"></i>
    <% end %>
  </li>
  <li>
    <%= link_to @supplier_payment.to_last, :class => 'notice-icon-button', :id => 'last', :title => I18n.t(:last) do %>
    <i class="icon-fast-forward"></i>
    <% end %>
  </li>
  <% end %>
  <% end %>
  <!-- Close -->
  <a class="close" href="<%= ag2_purchase.root_path %>">&#215;</a>
</ul>
