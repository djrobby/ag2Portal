<%= render '/layouts/ag2_admin/streettypesbreadcrumb' %>

<h3>Street Types</h3>
<div class="span7">
  <table class="table table-condensed table-striped">
    <thead>
      <tr>
        <th>Code</th>
        <th>Description</th>
        <th>Registered</th>
        <th></th>
        <th></th>
      </tr>
    </thead>
    <tbody>
      <% @street_types.each do |street_type| %>
      <tr>
        <td><%= link_to street_type.street_type_code, street_type %></td>
        <td><%= street_type.street_type_description.titleize %></td>
        <td><%= street_type.created_at.to_date %></td>
        <td><%= link_to 'Edit', edit_street_type_path(street_type), :class => 'btn btn-mini' %></td>
        <td><%= link_to("Delete", street_type, :data => { :confirm => "Are you sure?" }, :method => :delete, :class => 'btn btn-mini') %></td>
      </tr>
      <% end %>
    </tbody>
  </table>
  <%= link_to 'New Street type', new_street_type_path, :class => 'btn btn-primary' %>
  <br />
  <br />
  <%= link_to 'Go to ag2Towns', towns_path %> |
  <%= link_to 'Go to ag2Provinces', provinces_path %> |
  <%= link_to 'Back to ag2Admin', ag2_admin.root_path %>
</div>
