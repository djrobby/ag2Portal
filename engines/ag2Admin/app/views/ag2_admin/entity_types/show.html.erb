<%= render '/layouts/ag2_admin/entitytypesbreadcrumb' %>

<h3><%=t '.title' %></h3>
<table class="table">
  <tbody>
    <tr>
      <td>Id:</td>
      <td><%= @entity_type.id %></td>
      <td><%=t :"activerecord.attributes.entity_type.name" %>:</td>
      <td><%= @entity_type.name.titleize %></td>
    </tr>
    <%= render :partial => '/layouts/createdat_updatedat', :locals => { :ivar => @entity_type } %>
    <%= render :partial => '/layouts/createdby_updatedby', :locals => { :ivar => @entity_type } %>
  </tbody>
</table>

<%= render :partial => '/layouts/crud/update', :locals => { :model => EntityType, :path => edit_entity_type_path(@entity_type), :txt => I18n.t('ag2_admin.entity_types.show.btn_edit') } %>
<%= link_to I18n.t("ag2_admin.entity_types.back_to_main_page"), entity_types_path %>
