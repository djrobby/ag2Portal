<style type="text/css">
  .disabled a:hover{
    color: #aaa;
    cursor: not-allowed !important;
  }
  .disabled:hover{
    color: #fff;
  }
  .disabled:focus{
    color: #fff;
    background-color: white;
  }
  #billing-modal{
    width: 880px !important;
    left: 40% !important;
  }
  #billing-modal-cancellation{
    width: 880px !important;
