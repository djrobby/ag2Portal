<%= render '/layouts/ag2_gest/watersupplycontractsbreadcrumb' %>

<h3><%=t '.title' %></h3>
<table class="table odd_even_table">
  <tbody>
    <tr>
      <td>Id:</td>
      <td colspan="3"><%= @water_supply_contract.id %></td>
    </tr>
    <tr>
      <td><%=t 'activerecord.attributes.water_supply_contract.contracting_request_id' %>:</td>
