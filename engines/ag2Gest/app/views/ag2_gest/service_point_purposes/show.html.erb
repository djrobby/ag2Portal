<%= render '/layouts/ag2_gest/servicepointpurposebreadcrumb' %>

<h3><%= t('.title') %> <%=@service_point_purpose.name %></h3>
<table class="table">
  <tbody>
    <tr>
      <td><%= t('activerecord.attributes.service_point_purpose.id') %>:</td>
      <td><%= @service_point_purpose.try(:id) %></td>
      <td><%= t('activerecord.attributes.service_point_purpose.name') %>:</td>
      <td><%= @service_point_purpose.try(:name) %></td>
    </tr>
    	<%= render :partial => '/layouts/createdat_updatedat', :locals => { :ivar => @service_point_purpose } %>
  </tbody>
</table>

<%= render partial: '/layouts/crud/update_delete_new', locals:
{ model: ServicePointPurpose, update_path: edit_service_point_purpose_path(@service_point_purpose), update_txt: t('.btn_edit'),
delete_path: @service_point_purpose, delete_txt: t('.btn_delete'), msg: t("activerecord.models.service_point_purpose.one"), :txt => @service_point_purpose.id,
new_path: new_service_point_purpose_path, new_txt: t('ag2_gest.service_point_purposes.index.btn_new') } %>

<%= link_to I18n.t('ag2_gest.service_point_purposes.back_to_main_page'), service_point_purposes_path %>