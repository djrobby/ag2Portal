<% content_for :title, "aGestiona2 | ag2Subscribers" %>
<% content_for :icon, "/assets/pict/ag2/ag2Subscriber.png" %>
<% content_for :alt, "ag2Subscribers" %>

<ul class="breadcrumb">
  <li>
    <a href="<%= main_app.root_path %>"><i class="icon-home"></i></a><span class="divider">></span>
  </li>
  <li>
    <a href="<%= ag2_gest.root_path %>">ag2Gest</a><span class="divider">></span>
  </li>
  <% if @subscribers %>
    <li>
      <%=t "ag2_gest.breadcrumb.subscribers.items" %>
    </li>
  <% end %>
  <% if @subscriber %>
    <li>
      <a href="<%= subscribers_path %>"><%=t :"ag2_gest.breadcrumb.subscribers.items" %></a><span class="divider">></span>
    </li>
    <% if @breadcrumb == 'read' || @breadcrumb == 'update' %>
      <%=t :"ag2_gest.breadcrumb.subscribers.item" %>
    <% end %>
    <% if @breadcrumb == 'create' %>
      <li>
        <%= t :"breadcrumb.new" %>
      </li>
    <% end %>

  <% end %>
  <!-- Close -->
  <a class="close" href="<%= ag2_gest.root_path %>">&#215;</a>
</ul>