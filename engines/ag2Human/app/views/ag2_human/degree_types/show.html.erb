<%= render '/layouts/ag2_human/dtypesbreadcrumb' %>

<h3><%=t '.title' %></h3>
<table class="table">
  <tbody>
    <tr>
      <td>Id:</td>
      <td><%= @degree_type.id %></td>
      <td><%=t 'activerecord.attributes.degree_type.dt_code' %>:</td>
      <td><%= @degree_type.dt_code unless @degree_type.dt_code.nil? %></td>
    </tr>
    <tr>
      <td><%=t 'activerecord.attributes.degree_type.name' %>:</td>
      <td><%= @degree_type.name %></td>
      <td><%= t :"activerecord.attributes.degree_type.nomina_id" %>:</td>
      <td><%= @degree_type.nomina_id unless @degree_type.nomina_id.blank? %></td>
    </tr>
    <%= render :partial => '/layouts/createdat_updatedat', :locals => { :ivar => @degree_type } %>
    <%= render :partial => '/layouts/createdby_updatedby', :locals => { :ivar => @degree_type } %>
  </tbody>
</table>

<%= render partial: '/layouts/crud/update_delete_new', locals:
{ model: DegreeType, update_path: edit_degree_type_path(@degree_type), update_txt: t('.btn_edit'),
delete_path: @degree_type, delete_txt: t('.btn_delete'), msg: t("activerecord.models.degree_type.one"), txt: @degree_type.name,
new_path: new_degree_type_path, new_txt: t('ag2_human.degree_types.index.btn_new') } %>

<% if (@workers.count > 0) %>
<% session[:return_to] = I18n.t('ag2_human.degree_types.show.return_to') %>
<% session[:return_to_url] = "#{request.protocol}#{request.host_with_port}#{request.fullpath}" %>
<h4><%=t '.subtable_title' %></h4>
<table class="table table-condensed table-striped">
  <thead>
    <%= render '/layouts/ag2_human/workersheader' %>
  </thead>
  <tbody>
    <%= render :partial => '/layouts/ag2_human/workers', :locals => { :workers => @workers } %>
  </tbody>
</table>
<%= render :partial => '/layouts/pagination', :locals => { :ivar => @workers } %>
<% end %>

<%= link_to I18n.t('ag2_human.degree_types.back_to_main_page'), degree_types_path %>
